{
  "root": "apps/web",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "buildCommand": "cd ../.. && pnpm build --filter=@brainrot/web",
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "devCommand": "next dev --turbopack",
  "ignoreCommand": "git diff HEAD^ HEAD --quiet -- .",
  "github": {
    "silent": true
  }
}